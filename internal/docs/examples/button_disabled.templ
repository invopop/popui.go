package examples

import (
	"github.com/invopop/popui.go"
	"github.com/invopop/popui.go/props"
)

templ ButtonDisabledExample() {
	@popui.ButtonGroup() {
		@popui.Button(props.Button{Disabled: true}) {
			Disabled Default
		}
		@popui.Button(props.Button{Variant: props.ButtonVariantPrimary, Disabled: true}) {
			Disabled Primary
		}
		@popui.Button(props.Button{Variant: props.ButtonVariantDanger, Disabled: true}) {
			Disabled Danger
		}
	}
}
