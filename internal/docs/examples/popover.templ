package examples

import (
	"github.com/invopop/popui.go"
	"github.com/invopop/popui.go/props"
)

templ PopoverExample() {
	@popui.Popover(props.Popover{
		ID:          "popover-default",
		ButtonLabel: "···",
	}) {
		@popui.PopoverItem() {
			Edit
		}
		@popui.PopoverItem() {
			Duplicate
		}
		@popui.PopoverItem() {
			Delete
		}
	}
}
