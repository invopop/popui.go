package examples

import (
	"github.com/invopop/popui.go"
	"github.com/invopop/popui.go/props"
)

templ PopoverRightAlignedExample() {
	@popui.Popover(props.Popover{
		ID:          "popover-right",
		ButtonLabel: "···",
		RightAlign:  true,
	}) {
		@popui.PopoverItem() {
			Settings
		}
		@popui.PopoverItem() {
			Profile
		}
		@popui.PopoverItem() {
			Logout
		}
	}
}
