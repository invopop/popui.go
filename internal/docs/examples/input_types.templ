package examples

import (
	"github.com/invopop/popui.go"
	"github.com/invopop/popui.go/props"
)

templ InputTypesExample() {
	@popui.Input(props.Input{
		Type:        "email",
		Placeholder: "email@example.com",
	})
	@popui.Input(props.Input{
		Type:        "password",
		Placeholder: "Enter password",
	})
	@popui.Input(props.Input{
		Type: "date",
	})
}
